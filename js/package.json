{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "yarn run lint && webpack --config ./webpack.config.js  --mode=development",
    "build-prod": "yarn run lint && webpack --config ./webpack.config.js --mode=production",
    "analyze": "webpack --config ./webpack.config.js --mode=production --analyze",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --fix",
    "format": "yarn run format:prettier",
    "format:prettier": "yarn run prettier --write './src/**/*.{js,ts,tsx,md}'"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "corejs": 3,
          "useBuiltIns": "usage"
        }
      ]
    ],
    "plugins": [
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/plugin-proposal-class-properties",
        {
          "loose": true
        }
      ],
      [
        "emotion"
      ]
    ]
  },
  "browserslist": [
    "last 1 chrome versions",
    "last 1 edge versions",
    "last 1 firefox versions",
    "last 1 safari versions",
    "last 1 and_chr versions",
    "last 1 ios_saf versions",
    "ie 11"
  ],
  "dependencies": {
    "@atlassian/wrm-react-i18n": "^0.8.0",
    "@emotion/cache": "^10.0.29",
    "@emotion/core": "^10.1.1",
    "@emotion/styled": "^10.0.27",
    "axios": "^0.21.1",
    "dompurify": "^2.2.6",
    "emotion": "^10.0.27",
    "mobx": "^6.1.5",
    "mobx-react": "^7.1.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/plugin-proposal-decorators": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@types/dompurify": "^2.2.1",
    "@types/node": "^14.14.22",
    "@types/react": "^16.14.3",
    "@types/react-dom": "^16.9.10",
    "@types/webpack": "^4.41.26",
    "@typescript-eslint/eslint-plugin": "^4.14.2",
    "@typescript-eslint/parser": "^4.14.2",
    "@wordpress/eslint-plugin": "^8.0.2",
    "atlassian-webresource-webpack-plugin": "^4.9.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-emotion": "^10.0.33",
    "browserslist": "^4.16.3",
    "caniuse-lite": "^1.0.30001183",
    "clean-webpack-plugin": "^3.0.0",
    "core-js": "^3.8.3",
    "css-loader": "^5.0.1",
    "directory-named-webpack-plugin": "^4.0.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-compat": "^3.9.0",
    "eslint-plugin-flowtype": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jam3": "^0.2.3",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prefer-object-spread": "^1.2.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "file-loader": "^6.2.0",
    "inspectpack": "^4.6.1",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "style-loader": "^2.0.0",
    "terser-webpack-plugin": "^4.2.3",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3",
    "url-loader": "^4.1.1",
    "webpack": "^4.46.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-sources": "^2.2.0",
    "webpack-stream": "^6.1.2",
    "webpackbar": "^4.0.0"
  },
  "resolutions": {
    "fsevents": "^2.1.3",
    "react": "16.14.0",
    "node-fetch": "^2.6.1"
  },
  "peerDependencies": {
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "styled-components": "^3.2.6"
  }
}
